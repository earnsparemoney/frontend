{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:client": "vue-cli-service serve",
    "dev:server": "cross-env BUILD_TARGET=server NODE_ENV=development DEV=dev nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "vue-cli-service build",
    "build:server": "cross-env BUILD_TARGET=server vue-cli-service build --no-clean",
    "build:modern": "vue-cli-service build --modern",
    "build": "npm run build:client && npm run build:server",
    "build:docker": "docker-compose up --build",
    "docker": "docker-compose up",
    "report": "vue-cli-service build --report",
    "inspect": "vue-cli-service inspect > output.js",
    "lint": "vue-cli-service lint",
    "start": "nodemon server/server.js"
  },
  "dependencies": {
    "ant-design-vue": "^1.3.10",
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "cross-env": "^5.2.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "commitizen": "^3.1.1",
    "concurrently": "^4.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^2.4.1",
    "koa": "^2.7.0",
    "koa-proxy": "^1.0.0-alpha.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.19.1",
    "semver": "^6.1.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ua-parser-js": "^0.7.20",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
